{
  "name": "Alcarin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash.debounce": "4.0.8",
    "node-sass-chokidar": "1.3.0",
    "phoenix": "1.3.0",
    "prop-types": "15.6.1",
    "ramda": "0.25.0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-scripts": "1.1.4",
    "redux": "4.0.0",
    "reduxsauce": "1.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-console": "^1.0.4",
    "@storybook/addon-knobs": "^3.4.7",
    "@storybook/addon-links": "^3.4.6",
    "@storybook/addon-notes": "^3.4.7",
    "@storybook/addons": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.74.0",
    "npm-run-all": "^4.1.3",
    "prettier": "1.13.5",
    "react-test-renderer": "^16.4.0",
    "source-map-explorer": "^1.5.0"
  },
  "scripts": {
    "start": "npm-run-all -p _watch-css _start-js",
    "build": "npm-run-all _build-css _build-js",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint ./src",
    "flow": "flow",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "source-map-explorer build/static/js/main.*",
    "_build-js": "react-scripts build",
    "_build-css": "node-sass-chokidar src/ -o src/",
    "_start-js": "react-scripts start",
    "_watch-css": "npm run _build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  }
}
